# Discord Bot Token (required)
DISCORD_TOKEN=your_discord_bot_token_here

# Logging Level (optional, default: info)
# Possible values: error, warn, info, debug
LOG_LEVEL=info

# Cleanup Interval in milliseconds (optional, default: 604800000 - 7 days)
# Time after which inactive Discord clients will be destroyed to free resources
CLEANUP_INTERVAL=604800000

# Maximum number of retries for operations (optional, default: 3)
MAX_RETRIES=3

# Base delay in milliseconds for retry operations (optional, default: 1000)
RETRY_BASE_DELAY=1000
